/* MainWindow */

QMainWindow {
  width: 1920px;
  height: 1080px;
}

/* container */
QWidget#container {
  width: 90%;
  height: 90%;
  margin: 0 auto;
  background-color: #f0f0f0; /* Couleur de fond pour le conteneur */
}

/* card */
QWidget#card {
  background-color:  #4CAF50 ;
  padding: 20px;
  border-radius: 16px; /* Border-radius plus prononcé */
  box-shadow: 0 8px 16px rgba(0, 0, 0, 0.1); /* Légère ombre */
}

/* titleLabel */
QLabel#titleLabel {
  text-align: center;
  color: #333;
	font: 18pt &quot;Courier New&quot;;
}

/* form */
QWidget#form {
  width: 100%;
}

/* textInput */
QLineEdit#textInput {
  padding: 12px;
  margin-bottom: 16px;
  border: 2px solid #ccc; /* Couleur de bordure plus douce */
  border-radius: 20px;
  transition: border-color 0.3s ease-in-out;
  outline: none;
  color: #333;
}

QLineEdit#textInput:focus {
  border-color: #a5783d;
}

/* submitButton */
QPushButton#submitButton {
  background-color: #007a4d; /* Vert stylé associé aux assurances */
  color: #ffeacc;
  padding: 12px;
  border: none;
  border-radius: 18px;
  cursor: pointer;
  transition: background-color 0.3s ease-in-out;
}

QPushButton#submitButton:hover {
  background-color: #005734; /* Variation de couleur au survol */
}
