          <string notr="true">/* Style de base pour le QPushButton */
QPushButton {
    background-color: #3f9142; /* Vert primaire du thème d'assurance */
    color: #ffffff; /* Texte blanc */
    border: 3px solid #4CAF50; /* Bordure verte */
    border-radius: 25px; /* Coins arrondis */
    padding: 5px 10px; /* Espacement interne */
}

/* Style du QPushButton lorsqu'il est survolé */
QPushButton:hover {
    background-color: #45a049; /* Vert plus foncé au survol */
}

/* Style du QPushButton lorsqu'il est pressé (clic) */
QPushButton:pressed {
    background-color: #3c8942; /* Vert encore plus foncé au clic */
}

/* Style du QPushButton lorsqu'il est en focus (tabulation) */
QPushButton:focus {
    outline: none; /* Supprimer le contour par défaut */
    border: 2px solid #3c8942; /* Bordure plus foncée lorsqu'en focus */
}

/* Style du QPushButton lorsqu'il est désactivé */
QPushButton:disabled {
    background-color: #b0b0b0; /* Fond gris clair pour les boutons désactivés */
    color: #707070; /* Couleur du texte grise pour les boutons désactivés */
    border: 2px solid #b0b0b0; /* Bordure grise pour les boutons désactivés */
}

/* Style général pour les widgets lorsqu'ils sont désactivés */
QWidget:disabled {
    background-color: #f0f0f0; /* Fond gris clair pour les widgets désactivés */
    color: #a0a0a0; /* Couleur du texte grise pour les widgets désactivés */
}
